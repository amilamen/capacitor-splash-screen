{
  "name": "@willsub/capacitor-splashscreen",
  "version": "1.0.0",
  "description": "Provides access to the native biometric auth APIs for Capacitor apps",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check --plugin=./node_modules/prettier-plugin-java && npm run swiftlint -- lint",
    "prettier": "prettier --check --plugin=./node_modules/prettier-plugin-java \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Aparajita Fishman",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^2.4.1",
    "@capacitor/core": "^2.4.1",
    "@capacitor/ios": "^2.4.1",
    "@ionic/prettier-config": "^1.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "prettier": "^2.1.1",
    "prettier-plugin-java": "^0.8.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "swiftlint": "^1.0.1",
    "typescript": "~4.0.2"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "WillsubNpmCapacitorSplashscreen.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/willsub/capacitor-splashscreen.git"
  },
  "bugs": {
    "url": "https://github.com/willsub/capacitor-splashscreen.git/issues"
  }
}
